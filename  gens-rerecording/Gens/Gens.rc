// Menu
//

RAMWATCH_MENU MENU 
BEGIN
    POPUP "File"
    BEGIN
        MENUITEM "&New list",                   RAMMENU_FILE_NEW
        MENUITEM "&Open...",                    RAMMENU_FILE_OPEN
        MENUITEM "&Save",                       RAMMENU_FILE_SAVE
        MENUITEM "Sa&ve As...",                 RAMMENU_FILE_SAVEAS
        MENUITEM "&Append file...",             RAMMENU_FILE_APPEND
        MENUITEM "Recent",                      RAMMENU_FILE_RECENT
        MENUITEM SEPARATOR
        MENUITEM "Auto-&load",                  RAMMENU_FILE_AUTOLOAD
        MENUITEM SEPARATOR
        MENUITEM "&Close\tAlt F4",              RAMMENU_FILE_CLOSE
    END
    POPUP "Watches"
    BEGIN
        MENUITEM "New Watch",                   RAMMENU_WATCHES_NEWWATCH
        MENUITEM "Edit Watch",                  RAMMENU_WATCHES_EDITWATCH
        MENUITEM "Remove Watch",                RAMMENU_WATCHES_REMOVEWATCH
        MENUITEM "Duplicate Watch",             RAMMENU_WATCHES_DUPLICATEWATCH
        MENUITEM SEPARATOR
        MENUITEM "Move Up",                     RAMMENU_WATCHES_MOVEUP
        MENUITEM "Move Down",                   RAMMENU_WATCHES_MOVEDOWN
    END
END


